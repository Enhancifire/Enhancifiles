{
  "name": "coc-simple-react-snippets",
  "displayName": "Simple React Snippets",
  "description": "Dead simple React snippets you will actually use",
  "version": "1.1.0",
  "author": "Eliaz Bobadilla <eliaz.bobadilladev@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/UltiRequiem/coc-simple-react-snippets.gi5"
  },
  "keywords": [
    "React",
    "React JS",
    "React Snippets",
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.80"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescritreact"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets-ts.json"
      }
    ]
  }
}
