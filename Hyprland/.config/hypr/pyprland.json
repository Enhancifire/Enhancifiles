{
  "pyprland": {
    "plugins": [
      "scratchpads"
    ]
  },
  "scratchpads": {
    "term": {
      "command": "kitty --class dropterm --title drop",
      "animation": "fromTop",
      "margin": 50,
      "unfocus": "hide"
    },
    "files": {
      "command": "kitty --class ranger ranger",
      "animation": "fromTop",
      "unfocus": "hide"
    }
  }
}
